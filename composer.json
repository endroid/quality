{
    "name": "endroid/quality",
    "description": "Endroid Quality",
    "keywords": ["endroid", "quality"],
    "homepage": "https://github.com/endroid/quality",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "symfony/var-dumper": "^3.4||^4.4||^5.0"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "bin": [
        "bin/code-quality",
        "bin/check",
        "bin/unit-test",
        "bin/functional-test"
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
